{
  "name": "jupyterlab_nbprovenance",
  "version": "0.0.1",
  "description": "JupyterLab Notebook Provenance",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab extension"
  ],
  "homepage": "https://github.com/VisualStorytelling/jupyterlab_nbprovenance",
  "license": "Apache 2.0",
  "authors": [
    "Netherlands eScience Center",
    "The Caleydo Team"
  ],
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/VisualStorytelling/jupyterlab_nbprovenance"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "prepublishOnly": "npm run build",
    "extension:install": "jupyter labextension install jupyterlab_nbprovenance",
    "extension:uninstall": "jupyter labextension uninstall  jupyterlab_nbprovenance",
    "extension:enable": "jupyter labextension enable jupyterlab_nbprovenance",
    "extension:disable": "jupyter labextension disable jupyterlab_nbprovenance"
  },
  "private": true,
  "workspaces": {
    "packages": [
      "ProvBook"
    ]
  },
  "dependencies": {
    "@jupyterlab/application": "2.2.1",
    "@jupyterlab/console": "2.2.1",
    "@jupyterlab/mainmenu": "0.7.4",
    "@jupyterlab/notebook": "2.2.1",
    "@lumino/messaging": "^1.4.3",
    "@lumino/widgets": "^1.13.4",
    "@visualstorytelling/provenance-core": "1.1.5",
    "@visualstorytelling/provenance-tree-visualization-react": "1.0.3",
    "jupyterlab": "0.17.5",
    "node-sass": "4.9.3",
    "react": "16.9.0",
    "react-dom": "16.9.0"
  },
  "devDependencies": {
    "tslint": "5.11.0",
    "typescript": "3.0.3",
    "watch": "1.0.2"
  },
  "jupyterlab": {
    "extension": true
  }
}
